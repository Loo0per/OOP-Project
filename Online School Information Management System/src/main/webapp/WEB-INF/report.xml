<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>report</servlet-name>
		<servlet-class>com.report.ReportInsert</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReportInsert</servlet-name>
		<url-pattern>/ReportInsert</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>reportDisplay</servlet-name>
		<servlet-class>com.report.SearchStudentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchStudentServlet</servlet-name>
		<url-pattern>/SearchStudentServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>reportUpdate</servlet-name>
		<servlet-class>com.report.UpdateStudentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateStudentServlet</servlet-name>
		<url-pattern>/UpdateStudentServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>reportDelete</servlet-name>
		<servlet-class>com.report.DeleteStudentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteStudentServlet</servlet-name>
		<url-pattern>/DeleteStudentServlet</url-pattern>
	</servlet-mapping>
	
	
	
</web-app>	